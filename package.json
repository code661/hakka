{
  "private": true,
  "name": "graphql-api-starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "EGOIST <0x142857@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main",
    "dev": "rm -rf dist && yarn build --watch",
    "build": "rm -rf dist && rollup -c rollup.server.js",
    "migrate:save": "yarn build && typeorm migration:generate -n",
    "migrate:deploy": "typeorm migration:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "apollo-server-express": "2.19.0",
    "body-parser": "1.19.0",
    "class-validator": "0.12.2",
    "cookie": "0.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.9.7",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "graphql": "15.4.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "12.0.3",
    "ms": "^2.1.3",
    "nanoid": "3.1.20",
    "passport": "0.4.1",
    "passport-github": "1.1.0",
    "passport-google-oauth20": "2.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.2.29"
  },
  "devDependencies": {
    "@egoist/prettier-config": "0.1.0",
    "@types/cookie": "0.4.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/markdown-it": "12.0.0",
    "@types/ms": "^0.7.31",
    "@types/passport": "1.0.4",
    "@types/passport-github": "1.1.5",
    "@types/passport-google-oauth20": "2.0.4",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.2.4",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "rollup": "2.35.1",
    "rollup-plugin-spawn": "1.0.1",
    "rollup-plugin-typescript2": "0.29.0",
    "typescript": "4.1.3"
  }
}
